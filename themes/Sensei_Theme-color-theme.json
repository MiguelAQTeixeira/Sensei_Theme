{
	"name": "Sensei_Theme",
	"colors": {
		/*
		--- TitleBar
		*/
		"titleBar.activeBackground": "#161D27",
		"titleBar.activeForeground": "#63C7B2",
		"titleBar.inactiveBackground": "#1E2734",
		"titleBar.inactiveForeground": "#63C7B280",
		/*
		--- Editor
		*/
		"editor.background": "#1E2734",
		"editor.foreground": "#eeffff",
		//Cor do número das linhas laterais ao código
		"editorLineNumber.activeForeground": "#63C7B2",
		"editorLineNumber.foreground": "#8f9088",
		"editorCursor.background": "#ffffff",
		"editorCursor.foreground": "#FFD261",
		"editor.selectionBackground": "#63C7B233",
		"editor.selectionHighlightBackground": "#63C7B21a",
		/*
		--- Breadcrumbs
		*/
		"breadcrumb.background": "#1E2734",
		"breadcrumb.foreground": "#eeffff",
		"breadcrumb.focusForeground": "#63C7B2",
		"breadcrumb.activeSelectionForeground": "#FFD261",
		/*
		--- Editor groups & tabs
		*/
		/*"editorGroup.border": "#ff0000",
		"editorGroup.emptyBackground": "#000000",*/
		"tab.activeBackground": "#1E2734",
		"tab.activeForeground": "#eeffff",
		"tab.inactiveBackground": "#1E2734",
		"tab.inactiveForeground": "#eeffff80",
		"tab.activeBorder": "#63C7B2",
		/*
		--- Buttons
		*/
		"button.background": "#FFD261",
		"button.foreground": "#161D27",
		/*
		--- Menu
		*/
		"menu.background": "#161D27",
		"menu.foreground": "#FFD261",
		"menu.selectionBackground": "#2C3A4E",
		/*
		--- Sidebar
		*/
		"sideBar.background": "#161D27",
		"sideBarTitle.foreground": "#ade8f4",
		"sideBarSectionHeader.background": "#1E2734",
		"sideBarSectionHeader.foreground": "#eeffff",
		"sideBar.dropBackground": "#ff0000",//não funciona?
		/*
		--- Minimap
		*/
		"minimap.background": "#1E2734",
		"minimap.selectionHighlight": "#1E2634",
		"minimapSlider.background": "#ade8f440",
		"minimapSlider.activeBackground": "#ade8f41a",
		/*
		--- Activity Bar
		*/
		"activityBar.background": "#0F131A",
		//Cor das notificações dos ícones na activity bar
		"activityBarBadge.foreground": "#000000",
		"activityBarBadge.background": "#63C7B2",
		//Borda lateral nos ícones da activity bar
		"activityBar.activeBorder": "#FFD261",
		"activityBar.activeBackground": "#0F131A",
		"activityBar.dropBorder": "#FFD261",
		/*
		--- Progress Bar
		*/
		"progressBar.background": "#161D27",
		/*
		--- Panel 
		*/
		"panel.background": "#0F131A",
		"panelTitle.activeForeground": "#FFD261",
		"panelTitle.inactiveForeground": "#eeffff",
		"panelSectionHeader.background": "#ade8f4",
		/*
		--- Status bar
		*/
		"statusBar.background": "#63C7B2",
		"statusBar.foreground": "#0F131A",
		/*
		--- Scrollbar
		*/
		"scrollbarSlider.activeBackground": "#ade8f4",
		"scrollbarSlider.hoverBackground": "#ade8f41a",
		"scrollbar.shadow": "#0F131A",
		/*
		--- Terminal
		*/
		"terminal.background": "#252F41",
		"terminal.foreground": "#ffffff",
		"terminal.selectionBackground": "#63C7B233",
		"terminalCursor.foreground": "#FFD261",
		"terminalCursor.background": "#ffffff",
		"terminal.dropBackground": "#63C7B233",
		"terminal.tab.activeBorder": "#63C7B2"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#546E7A"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#edf6f9"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#90e0ef"
			}
		},
		{
